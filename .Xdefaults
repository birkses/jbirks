URxvt.font: xft:DejaVu Sans Mono-13:book:autohint=true:hinting=true
URxvt.boldFont: xft:DejaVu Sans Mono-13:book:autohint=true:hinting=true
URxvt*geometry:         53x20
URxvt*scrollBar:        false
URxvt*saveLines:        30000
URxvt*termName:         rxvt-unicode

URxvt*dynamicColors:    on
URxvt.background:       black
URxvt.foreground:       #ffde95
URxvt.scrollBar:        false
URxvt.tintColor:        white
URxvt.fading:           15
URxvt.depth:            32
URxvt.fadeColor:        black
URxvt.shading:          15
URxvt.inheritPixmap:    true
URxvt.pointerColor:     black
URxvt.pointerColor2:    white

!urxvt font settings
URxvt.letterSpace: -2
URxvt*cursorColor:  #FFFFFF

!black
*color0: #262729
*color8: #555544
!red
*color1: #CE000B
*color9: #f57900
!green
*color2: #668800
*color10: #88aa22
!brown/yellow
*color3: #B88B10
*color11: #FF8C00
!blue
*color4: #5485a4
*color12: #85A8C0
!magenta
*color5: #85608b
*color13: #ad7fa8
!cyan
*color6: #807855
*color14: #99916A
!white
*color7: #767779
*color15: #b6b8b9
